@model List<EmailLogViewModel><div class="table-responsive mt-3 d-none d-md-block">    <table class="table admin-dashboard">        <thead class="table-head table-secondary align-middle">            <tr >                <th>Recipient</th>                <th>Action</th>                <th>Role Name</th>                <th>Email Id</th>                <th>Create Date</th>                <th>Sent Date</th>                <th>Sent</th>                <th>Sent Tries</th>                <th>Confirmation Number</th>            </tr>        </thead>        <tbody class="align-middle" id="dashboard-table-body">            @for(int i=0;i<Model.Count;i++)            {                <tr>                    <td>@Model[i].Recipient</td>                    <td>@Model[i].Action</td>                    <td>@Model[i].RoleId</td>                    <td>@Model[i].Email</td>                    <td>@Model[i].CreateDate</td>                    <td>@Model[i].SentDate</td>                    <td>@Model[i].Sent</td>                    <td>@Model[i].SentTries</td>                    <td></td>                </tr>            }        </tbody>    </table></div>