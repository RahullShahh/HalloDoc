@{    TempData.Keep("UserName");}<header class="">    <div class="d-flex mx-3 align-items-center justify-content-between">        <div class="d-flex justify-content-center align-items-center">            <div>                <button class="btn btn-primary d-md-none d-block" type="button" data-bs-toggle="offcanvas" data-bs-target="#MenuButton" aria-controls="MenuButton">                    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-list" viewBox="0 0 16 16">                        <path fill-rule="evenodd" d="M2.5 12a.5.5 0 0 1 .5-.5h10a.5.5 0 0 1 0 1H3a.5.5 0 0 1-.5-.5m0-4a.5.5 0 0 1 .5-.5h10a.5.5 0 0 1 0 1H3a.5.5 0 0 1-.5-.5m0-4a.5.5 0 0 1 .5-.5h10a.5.5 0 0 1 0 1H3a.5.5 0 0 1-.5-.5" />                    </svg>                </button>            </div>            <div>                <img src="/SRS Screen Shorts/HalloDoc LOGO.png" alt="logo_icon" height="100px">            </div>            <div>                <button class="btn btn-primary d-md-none d-block" type="button" data-bs-toggle="offcanvas" data-bs-target="#InfoButton" aria-controls="InfoButton">                    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-three-dots-vertical" viewBox="0 0 16 16">                        <path d="M9.5 13a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0m0-5a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0m0-5a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0" />                    </svg>                </button>            </div>        </div>        <div class="d-md-block d-none">            <span class="mx-1">Welcome <b>@TempData["UserName"]</b></span>            <a asp-controller="Provider" asp-action="Logout" class="btn btn-outline-info shadow-none night-mode-btn">                <span>Logout</span>            </a>            <button onclick="darkMode()" id="darkmodebutton" class="btn btn-outline-info shadow-none night-mode-btn mx-2">                <svg xmlns="http://www.w3.org/2000/svg" enable-background="new 0 0 24 24" height="24px" viewBox="0 0 24 24" width="24px">                    <rect fill="none" height="24" width="24" />                    <path d="M9.37,5.51C9.19,6.15,9.1,6.82,9.1,7.5c0,4.08,3.32,7.4,7.4,7.4c0.68,0,1.35-0.09,1.99-0.27C17.45,17.19,14.93,19,12,19 c-3.86,0-7-3.14-7-7C5,9.07,6.81,6.55,9.37,5.51z M12,3c-4.97,0-9,4.03-9,9s4.03,9,9,9s9-4.03,9-9c0-0.46-0.04-0.92-0.1-1.36 c-0.98,1.37-2.58,2.26-4.4,2.26c-2.98,0-5.4-2.42-5.4-5.4c0-1.81,0.89-3.42,2.26-4.4C12.92,3.04,12.46,3,12,3L12,3z" />                </svg>            </button>        </div>        <div class="offcanvas offcanvas-start d-md-none d-block" data-bs-scroll="true" tabindex="-1" id="InfoButton" aria-labelledby="InfoButton">            <div class="offcanvas-header">                <span class="mx-1">Welcome <b>@TempData["UserName"]</b></span>            </div>            <div class="offcanvas-body">                <a asp-controller="Provider" asp-action="Logout" class="btn btn-outline-info shadow-none night-mode-btn">                    <span>Logout</span>                </a>                <button onclick="darkMode()" id="darkmodebutton" class="btn btn-outline-info shadow-none night-mode-btn mx-2">                    <svg xmlns="http://www.w3.org/2000/svg" enable-background="new 0 0 24 24" height="24px" viewBox="0 0 24 24" width="24px">                        <rect fill="none" height="24" width="24" />                        <path d="M9.37,5.51C9.19,6.15,9.1,6.82,9.1,7.5c0,4.08,3.32,7.4,7.4,7.4c0.68,0,1.35-0.09,1.99-0.27C17.45,17.19,14.93,19,12,19 c-3.86,0-7-3.14-7-7C5,9.07,6.81,6.55,9.37,5.51z M12,3c-4.97,0-9,4.03-9,9s4.03,9,9,9s9-4.03,9-9c0-0.46-0.04-0.92-0.1-1.36 c-0.98,1.37-2.58,2.26-4.4,2.26c-2.98,0-5.4-2.42-5.4-5.4c0-1.81,0.89-3.42,2.26-4.4C12.92,3.04,12.46,3,12,3L12,3z" />                    </svg>                </button>            </div>        </div></header><hr class="m-0"><div class="sticky-top opacity-100 z-2 headerBackground d-md-block d-none">    <ul class="nav nav-pills ps-4 shadow" id="myTab" role="tablist">        <li class="nav-item" role="presentation">            <a asp-controller="Provider" asp-action="ProviderDashboard" class="nav-link px-0 admin-tab" id="dashboard-tab">                <span class="tabHeader py-2 px-3">Dashboard</span>            </a>        </li>        <li class="nav-item" role="presentation">            <a asp-controller="Provider" asp-action="ProviderInvoicing" class="nav-link px-0 admin-tab" id="provider-location-tab">                <span class="tabHeader py-2 px-3">Invoicing</span>            </a>        </li>        <li class="nav-item" role="presentation">            <a asp-controller="Provider" asp-action="ProviderSiteProviderScheduling" class="nav-link px-0 admin-tab" id="profile-tab">                <span class="tabHeader py-2 px-3">Schedule</span>            </a>        </li>        <li class="nav-item" role="presentation">            <a asp-controller="Provider" asp-action="ProviderMyProfile" class="nav-link px-0 admin-tab" id="profile-tab">                <span class="tabHeader py-2 px-3">My Profile</span>            </a>        </li>    </ul></div><div class="offcanvas offcanvas-start d-md-none d-block" data-bs-scroll="true" tabindex="-1" id="MenuButton" aria-labelledby="offcanvasWithBothOptionsLabel">    <div class="offcanvas-header">        Menu    </div>    <div class="offcanvas-body">        <ul class="nav nav-pills ps-4 shadow" id="myTab" role="tablist">            <li class="nav-item" role="presentation">                <a asp-controller="Admin" asp-action="AdminDashboard" class="nav-link px-0 admin-tab" id="dashboard-tab">                    <span class="tabHeader py-2 px-3">Dashboard</span>                </a>            </li>            <li class="nav-item" role="presentation">                <a asp-controller="Admin" asp-action="ProviderLocation" class="nav-link px-0 admin-tab" id="provider-location-tab">                    <span class="tabHeader py-2 px-3">Provider Location</span>                </a>            </li>            <li class="nav-item" role="presentation">                <a asp-controller="Admin" asp-action="AdminProfile" class="nav-link px-0 admin-tab" id="profile-tab">                    <span class="tabHeader py-2 px-3">My Profile</span>                </a>            </li>            <li class="nav-item" role="presentation">                <a asp-controller="Admin" asp-action="Providers" class="nav-link px-0 admin-tab" id="provider-tab">                    <span class="tabHeader py-2 px-3">Providers</span>                </a>            </li>            <li class="nav-item" role="presentation">                <a asp-controller="Admin" asp-action="Partners" class="nav-link px-0 admin-tab" id="partner-tab">                    <span class="tabHeader py-2 px-3">Partners</span>                </a>            </li>            <li class="nav-item" role="presentation">                <a asp-controller="Admin" asp-action="Access" class="nav-link px-0 admin-tab" id="access-tab">                    <span class="tabHeader py-2 px-3">Access</span>                </a>            </li>            <li class="nav-item" role="presentation">                <a asp-controller="Admin" asp-action="Records" class="nav-link px-0 admin-tab" id="record-tab">                    <span class="tabHeader py-2 px-3">Records</span>                </a>            </li>        </ul>    </div></div>